   "source": [
    "def arrange_anagrams(strings):\n",
    "    sorted_strings = [\"\".join(sorted(s)) for s in strings]\n",
    "    return len(set(sorted_strings))==1\n",
    "\n",
    "strings = ['listen','silent','state','taste']\n",
    "\n",
    "arrange_anagrams(strings)"
   ]
